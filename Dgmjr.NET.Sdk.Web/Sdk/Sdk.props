  </PropertyGroup>
  <Import Project="Sdk.props" Sdk="Microsoft.NET.Sdk.Web" Condition="$(ImportMicrosoftNetSdkWeb)" />
  <Import Project="Sdk.props" Sdk="Dgmjr.NET.Sdk" />
</Project>
