<!-- This doesn't do anything right now, but the idea is to allow the programmer the option to buikd a project from its source code or use the NuGet package -->
<Project>
  <PropertyGroup>
    <TempFile>$([System.IO.Path]::GetTempFileName())</TempFile>
    <TempPropsFile>$(TempFile).props</TempPropsFile>
    <TempTargetsFile>$(TempFile).targets</TempTargetsFile>
    <BuildFromSource>false</BuildFromSource>
  </PropertyGroup>
</Project>
